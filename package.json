{
    "name": "dotenv-packed",
    "version": "1.2.2",
    "description": "Pack dotenv and some of its extensions into one.",
    "main": "dist/index.js",
    "scripts": {
        "build": "babel src --out-dir ./dist --delete-dir-on-start --minified --source-maps",
        "test": "nyc mocha --require @babel/register",
        "coverage": "nyc report --reporter=text-lcov | coveralls"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/linhntaim/dotenv-packed.git"
    },
    "keywords": [
        "dotenv",
        "packed",
        "dotenv-packed",
        "env",
        ".env",
        "environment",
        "variables",
        "parse",
        "expand",
        "conversion"
    ],
    "author": "Nguyen Tuan Linh <contact@linhntaim.com> (https://linhntaim.com)",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/linhntaim/dotenv-packed/issues"
    },
    "homepage": "https://github.com/linhntaim/dotenv-packed#readme",
    "dependencies": {
        "dotenv": "^8.2.0",
        "dotenv-conversion": "^1.6.2",
        "dotenv-expand": "^5.1.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.13.14",
        "@babel/core": "^7.13.14",
        "@babel/preset-env": "^7.13.12",
        "@babel/register": "^7.13.14",
        "chai": "^4.3.4",
        "coveralls": "^3.1.0",
        "mocha": "^8.3.2",
        "nyc": "^15.1.0"
    }
}
